<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.www.matrixdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CanvasDrawActivity" />
        <activity android:name=".CanvasTransformationActivity" />
        <activity android:name=".CanvasStackActivity" />
        <activity android:name=".CanvasComposeActivity" />
        <activity android:name=".TextDrawActivity" />
        <activity android:name=".BitmapDrawActivity" />
        <activity android:name=".MatrixActivity" />
        <activity android:name=".SelfDrawableActivity" />
        <activity android:name=".CommonImgEffectActivity"></activity>
    </application>

</manifest>